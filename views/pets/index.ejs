<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>Pet Central</title>
</head>
<body>
  <h1>Pet Central Kennel</h1>

  <table id="kennel">
    <tbody>
      <% pets.forEach(function(pet){ %>
        <tr>
          <td><%= pet.name %></td>
          <td><%= pet.type %></td>
          <td><a href="/pets/<%= p._id %>">Details</a></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
  
</body>
</html>